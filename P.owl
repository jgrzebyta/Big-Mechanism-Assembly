    </owl:Class>
    


    <!-- http://purl.bioontology.org/net/brunel/p#NegativeConflict -->

    <owl:Class rdf:about="&bmp;NegativeConflict">
        <rdfs:label>negative conflict</rdfs:label>
        <rdfs:subClassOf rdf:resource="&bmp;ProbabilityRelevantToLinkUncertainty"/>
        <rdfs:comment>The event is not in the model but is found to be true in the evidence.
Suggested action =&gt; add the event to the model if the value is high.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.bioontology.org/net/brunel/p#NegativeCorroboration -->

    <owl:Class rdf:about="&bmp;NegativeCorroboration">
        <rdfs:label>negative corroboration</rdfs:label>
        <rdfs:subClassOf rdf:resource="&bmp;ProbabilityRelevantToLinkUncertainty"/>
        <rdfs:comment>The event is not in the model and is found to be false in the evidence.
Action is not required.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.bioontology.org/net/brunel/p#PositiveConflict -->

    <owl:Class rdf:about="&bmp;PositiveConflict">
        <rdfs:label>positive conflict</rdfs:label>
        <rdfs:subClassOf rdf:resource="&bmp;ProbabilityRelevantToLinkUncertainty"/>
        <rdfs:comment>The event is in the model but is found to be false in the evidence.
Suggested action =&gt; remove the event from the model if the value is high.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.bioontology.org/net/brunel/p#PositiveCorroboration -->

    <owl:Class rdf:about="&bmp;PositiveCorroboration">
