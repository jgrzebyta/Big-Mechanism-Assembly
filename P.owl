        <dc:creator rdf:datatype="&xsd;anyURI">http://www.brunel.ac.uk/people/larisa-soldatova</dc:creator>
        <owl:versionInfo>2016-03-15</owl:versionInfo>
        <dc:title xml:lang="en">Probabilities Ontology</dc:title>
        <dc:subject xml:lang="en">uncertainty</dc:subject>
    </owl:Ontology>
    


